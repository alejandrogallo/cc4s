  (unrestricted 1)
] [
  (CoulombVertex FullCoulombVertex)
  HoleEigenEnergies ParticleEigenEnergies
].

CoulombVertexSingularVectors [
  FullCoulombVertex
  (fieldVariables 100)
  (reduction 0.5)
] [
  CoulombVertexSingularVectors
