- name: UegVertexGenerator
  in: 
    No: 19
    Nv: 232
    halfGrid: 0
    rs: 1
  out: 
    eigenEnergies: EigenEnergies
    coulombVertex: CoulombVertex

- name: DefineHolesAndParticles
  in:
    eigenEnergies: EigenEnergies
  out:
    slicedEigenEnergies: EigenEnergies

- name: SliceOperator
  in:
    slicedEigenEnergies: EigenEnergies
    operator: CoulombVertex
  out:
    slicedOperator: CoulombVertex

- name: VertexCoulombIntegrals
  in:
    slicedCoulombVertex: CoulombVertex
  out:
    coulombIntegrals: CoulombIntegrals

- name: CoupledCluster
  in:
    method: Ccsd
    integralsSliceSize: 56
    slicedEigenEnergies: EigenEnergies
    coulombIntegrals: CoulombIntegrals
    slicedCoulombVertex: CoulombVertex
    maxIterations: 20
    energyConvergence: 1.0E-8
    amplitudesConvergence: 1.0E-8
    mixer:
      type: DiisMixer
      maxResidua: 4
  out:
    energy: CcsdEnergy
    amplitudes: Amplitudes

- name: PerturbativeTriplesReference
  in:
    coulombIntegrals: CoulombIntegrals
    intermediateOnly: 1
    amplitudes: Amplitudes
    slicedEigenEnergies: EigenEnergies
  out:
    cTIntermediate: CtIntermediate


- name: Write
  in:
    fileName: "CtIntermediate.yaml"
    source: CtIntermediate
    binary: 1
  out: {}

- name: Write
  in:
    fileName: "Amplitudes.yaml"
    source: Amplitudes
    binary: 1
  out: {}

- name: Write
  in:
    fileName: "EigenEnergiesSliced.yaml"
    source: EigenEnergies
    binary: 1
  out: {}

- name: Write
  in:
    fileName: "CoulombIntegrals.yaml"
    source: CoulombIntegrals
    binary: 1
  out: {}

- name: PerturbativeTriples
  in:
    coulombIntegrals: CoulombIntegrals
    amplitudes: Amplitudes
    slicedEigenEnergies: EigenEnergies
  out:
    {}

